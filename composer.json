{
    "name": "intoxstudio/restrict-user-access",
    "type": "project",
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "RestrictUserAccess\\": "src/"
        },
        "files": [
        ]
    },
    "authors": [
        {
            "name": "Joachim Jensen",
            "email": "joachim@dev.institute"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "php": "^7.2 || ^8.0",
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpstan": "^2.1",
        "szepeviktor/phpstan-wordpress": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0"
    },
    "scripts": {
        "test:phpstan": "vendor/bin/phpstan analyse -c phpstan.neon --memory-limit=-1",
        "fix:cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php"
    }
}
